<%
/************** Short helper to convert values to human readable format ****************/
function mattHumanValue(data,value)
{
	if (data.unit == '%')
		return (100.0*value/data.ref).toFixed(1) + " %";
	else
		return mattHelper.humanReadableValue(value,data.unit);
}
%>

<% for(var i in data.entries.slice(range.from,range.count)) {%>
	<li><a href="javascript:document.getElementById('matt-func-list').matt.clickEntry(<%= i %>);">
		<div class="progress">
			<div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= (100.0 * data.entries[i].value) / data.ref %>%;">
				<span class="sr-only">60% Complete</span>
			</div>
		</div>
		<span class='size'><%= mattHumanValue(data,data.entries[i].value) %></span>
		<span class='func'><%= $('<div/>').text(data.entries[i].name).html() %></span>
	</a></li>
<% } %>