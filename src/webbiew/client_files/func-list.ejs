<% for(var i in data.entries) {%>
	<li><a href="javascript:javascript:document.getElementById('matt-editor').matt.updateFile('<%= data.entries[i].details.file %>',<%= data.entries[i].details.line == undefined ? 0 : data.entries[i].details.line %>);">
		<div class="progress" style='display:span;float:left;width:7%;'>
			<div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= (100.0 * data.entries[i].value) / data.ref %>%;">
				<span class="sr-only">60% Complete</span>
			</div>
		</div>
		<span style='width:7em;display: inline-block;text-align:right;padding-right:1em;'><%= mattHumanValue(data,data.entries[i].value) %></span>
		<span><%= data.entries[i].name %></span>
	</a></li>
<% } %>